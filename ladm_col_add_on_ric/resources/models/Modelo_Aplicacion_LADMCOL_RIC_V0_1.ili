       */
      !!@ ili2db.dispName = "Dirección"
      Direccion : BAG {1..*} OF LADM_COL_V3_1.LADM_Nucleo.ExtDireccion;
      /** Corresponde a una caracterización temática de las condiciones de predio que se está censando relacionada a aspectos físicos y jurídicos.
       */
      !!@ ili2db.dispName = "Condición del predio"
      Condicion_Predio : MANDATORY Modelo_Aplicacion_LADMCOL_RIC_V0_1.RIC_CondicionPredioTipo;
      /** Es la clasificación para fines estadísticos que se da a cada inmueble en su conjunto–terreno, construcciones o edificaciones-, en el momento de la identificación predial de conformidad con la actividad predominante que en él se desarrolle.
       */
      !!@ ili2db.dispName = "Destinación económica"
      Destinacion_Economica : Modelo_Aplicacion_LADMCOL_RIC_V0_1.RIC_DestinacionEconomicaTipo;
      /** Indica el tipo predio para efectos catastrales.
       */
      !!@ ili2db.dispName = "Tipo de predio"
      Tipo (EXTENDED) : MANDATORY Modelo_Aplicacion_LADMCOL_RIC_V0_1.RIC_PredioTipo;
      /** El avalúo catastral consiste en la determinación del valor de los predios, obtenido mediante investigación y análisis estadístico del mercado inmobiliario. ?
       * El avalúo catastral de cada predio se determinará por la adición de los avalúos parciales practicados independientemente para los terrenos y para las unidades de construcción en él comprendidos.?
       * El avalúo catastral corresponde a un porcentaje adoptado del avalúo comercial según el estudio económico, dichos avalúos se redondean a miles.
       */
      !!@ ili2db.dispName = "Avalúo catastral"
      Avaluo_Catastral : MANDATORY LADM_COL_V3_1.LADM_Nucleo.Peso;
      /** Una unidad orgánica catastral se divide en zonas: urbana y rural.
       */
      !!@ ili2db.dispName = "Zona"
      Zona : MANDATORY Modelo_Aplicacion_LADMCOL_RIC_V0_1.RIC_ZonaTipo;
      /** Estado de La información física, jurídica y económica, así como la resultante de los procedimientos de enfoque multipropósito entrarán en vigencia para efectos catastrales al momento de quedar en firme su inscripción o incorporación en las bases oficiales descritas en el  decreto 148 de 2020.
       */
      !!@ ili2db.dispName = "Vigencia de la actualización catastral"
      Vigencia_Actualizacion_Catastral : MANDATORY INTERLIS.XMLDate;
      !!@ ili2db.dispName = "Estado"
      Estado : MANDATORY Modelo_Aplicacion_LADMCOL_RIC_V0_1.RIC_EstadoTipo;
      !!@ ili2db.dispName = "Catastro"
      Catastro : Modelo_Aplicacion_LADMCOL_RIC_V0_1.RIC_CatastroTipo;
      UNIQUE Codigo_Homologado;
      !!@ name = "ValidacionValoresPredioTipo"
      MANDATORY CONSTRAINT Tipo <> #Ordenamiento_Territorial ;
      MANDATORY CONSTRAINT Tipo <> #Servicios_Publicos ;
      MANDATORY CONSTRAINT Tipo <> #Reservas_Naturales ;
      MANDATORY CONSTRAINT Tipo <> #Parques_Naturales ;
      MANDATORY CONSTRAINT Tipo <> #Amenazas_Riesgos ;
      MANDATORY CONSTRAINT Tipo <> #Servidumbre ;
      MANDATORY CONSTRAINT Tipo <> #Superficies_Agua ;
      MANDATORY CONSTRAINT Tipo <> #Transporte ;
    END RIC_Predio;

    /** Porción de tierra con una extensión geográfica definida.
     */
    !!@ ili2db.dispName = "(SINIC) Terreno"
    CLASS RIC_Terreno
    EXTENDS LADM_COL_V3_1.LADM_Nucleo.COL_UnidadEspacial =
      /** Área total del terreno resultante del levantamiento catastral.
       */
      !!@ ili2db.dispName = "Área de terreno"
      Area_Terreno : MANDATORY 0.0 .. 99999999999999.9 [LADM_COL_V3_1.m2];
      /** Área de terreno digital registrada en la base de datos catastral.
       */
      !!@ ili2db.dispName = "Área digital del gestor"
      Area_Digital_Gestor : 0.0 .. 99999999999999.9 [LADM_COL_V3_1.m2];
      /** Corresponde a la relación de puntos, líneas, polígono y volumen que en su conjunto tienen propiedades de espacio y estructura, que definen el terreno, es asignado por el sistema de captura.
       */
      !!@ ili2db.dispName = "Geometría"
      Geometria (EXTENDED) : MANDATORY ISO19107_PLANAS_V3_0.GM_MultiSurface3D;
    END RIC_Terreno;

    /** Clase que contiene los cambios que se presentan en los componentes físico, jurídico o económico de un predio, una vez han sido formados.
     */
    !!@ ili2db.dispName = "(SINIC) Tramite Catastral"
    CLASS RIC_TramiteCatastral =
      /** Para los efectos catastrales, las mutaciones se clasifican de acuerdo a lo definido en la resolución IGAC 1149 de 2021.
       */
      !!@ ili2db.dispName = "Clasificación de la Mutación Catastral"
      Clasificacion_Mutacion : MANDATORY Modelo_Aplicacion_LADMCOL_RIC_V0_1.RIC_MutacionTipo;
      /** Numero de la resolucion de la mutación expedida por el gestor catastral.
       */
      !!@ ili2db.dispName = "Número de la resolución"
      Numero_Resolucion : MANDATORY TEXT*30;
      /** Fecha de la resolucion de la mutación expedida por el gestor catastral.
       */
      !!@ ili2db.dispName = "Fecha de la resolución"
      Fecha_Resolucion : MANDATORY INTERLIS.XMLDate;
    END RIC_TramiteCatastral;

    /** Es cada conjunto de materiales consolidados dentro de un predio que tiene unas características específicas en cuanto a elementos constitutivos físicos y usos de la misma.
     */
    !!@ ili2db.dispName = "(SINIC) Unidad de Construcción"
    CLASS RIC_UnidadConstruccion
    EXTENDS LADM_COL_V3_1.LADM_Nucleo.COL_UnidadEspacial =
      /** Indica numéricamente la ubicación de la planta.
       */
      !!@ ili2db.dispName = "Ubicación de la planta"
      Planta_Ubicacion : MANDATORY 0 .. 300;
      /** Área total construida en la unidad de construcción.
       */
      !!@ ili2db.dispName = "Área construida de la planta"
      Area_Construida : MANDATORY 0.0 .. 99999999999999.9 [LADM_COL_V3_1.m2];
      /** Altura total de la planta.
       */
      !!@ ili2db.dispName = "Altura de la planta"
      Altura : 1 .. 1000 [INTERLIS.m];
      /** Corresponde a la relación de puntos, líneas, polígono y volumen que en su conjunto tienen propiedades de espacio y estructura, que definen la construcción, es asignado por el sistema de captura.
       */
      !!@ ili2db.dispName = "Geometría"
      Geometria (EXTENDED) : MANDATORY ISO19107_PLANAS_V3_0.GM_MultiSurface3D;
    END RIC_UnidadConstruccion;

    ASSOCIATION ric_construccion_unidadconstruccion =
      ric_unidadconstruccion -- {0..*} RIC_UnidadConstruccion;
      ric_construccion -<> {1} RIC_Construccion;
    END ric_construccion_unidadconstruccion;

    ASSOCIATION ric_ph_predio =
      ric_predio -- {1} RIC_Predio;
      ric_datos_ph -- {0..1} RIC_DatosPHCondominio;
    END ric_ph_predio;

    /** Relación entre unidades prediales y predios matrices bajo el régimen de propiedad horizontal y condominio.
     */
    ASSOCIATION ric_predio_copropiedad =
      /** Area privada de terreno matriz, adicional a la participación en el terreno común, según el coeficiente allí determinado.
       */
      sinic_unidad_predial -- {0..*} RIC_Predio;
      /** Unidades residenciales tales como edificios, conjuntos y tipos de urbanizaciones cerradas en las que se comparten áreas comunes y que tienen régimen de propiedad horizontal.
       */
      sinic_matriz -<> {0..1} RIC_Predio;
      /** Coeficiente de copropiedad de la unidad predial.
       */
      !!@ ili2db.dispName = "Coeficiente"
      Coeficiente : 0.0000000000 .. 1.0000000000;
    END ric_predio_copropiedad;

    ASSOCIATION ric_predio_gestorcatastral =
      ric_predio -- {1..*} RIC_Predio;
      ric_gestorcatastral -- {1} RIC_GestorCatastral;
    END ric_predio_gestorcatastral;

    ASSOCIATION ric_predio_informalidad =
      ric_predio_formal -- {0..*} RIC_Predio;
      ric_predio_informal -- {0..*} RIC_Predio;
    END ric_predio_informalidad;

    ASSOCIATION ric_predio_operadorcatastral =
      ric_predio -- {1..*} RIC_Predio;
      ric_operadorcatastral -- {1} RIC_OperadorCatastral;
    END ric_predio_operadorcatastral;

    ASSOCIATION ric_predio_tramitecatastral =
      ric_predio -- {0..*} RIC_Predio;
      ric_tramite_catastral -- {0..*} RIC_TramiteCatastral;
    END ric_predio_tramitecatastral;

    ASSOCIATION ric_unidadconstruccion_caracteristicasunidadconstruccion =
      ric_caracteristicasunidadconstruccion -- {1} RIC_CaracteristicasUnidadConstruccion;
      ric_unidadconsutruccion -- {1..*} RIC_UnidadConstruccion;
    END ric_unidadconstruccion_caracteristicasunidadconstruccion;

  END RIC;

END Modelo_Aplicacion_LADMCOL_RIC_V0_1.
